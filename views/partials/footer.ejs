<script>
var navBar = $("#topNav");
var hdrHeight = $(".top_bar").height();


//TOP NAV SCRIPT
$(window).scroll(function(){
  if($(this).scrollTop() > hdrHeight){
    navBar.addClass("navScrolled");
  }else{
    navBar.removeClass("navScrolled")
  }
})
</script>

